@import 'font_sans.css';

*{
	margin: 0;
	padding: 0;
	/*border: 0;*/
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */

}
body, html {
	height:100%;	
}
body
{
	color: #555;
	font: normal 1em Ubuntu, Arial,Helvetica,sans-serif;
	background: #ECEBE9; /*#EFEFEF;*/
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */
	text-align: center;
}

body * {
	display: flex;
	flex: auto;
			flex-direction: row;
			flex-wrap: wrap;
			justify-content: space-around;
			align-items: stretch;
}


h1, h2, h3, h4 {
	color: #aaa;
	text-shadow: 1px 1px #FFF;	
}
h1, h2, h3 {
	font-weight:bolder;
	letter-spacing:0.1em;
	margin: 0.3em 0em 0.3em 0em;	
}
h1 {
	font-size: 3em;
	font-weight: 700;
}
h2 {
	font-size: 2em;
	font-weight: 300;
}
h3 {
	font-size: 1.2em;
	font-weight: 300;
}
h4 {
	font-size: 0.8em;
	font-weight: 300;
	letter-spacing:1px;
	margin: 0em 0em;
	padding: 0em;
}

label {
	/*color: #000;*/
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}
	label input { 	padding-left: .3em; }

input, textarea, select, button {
	font-size: 1em;
}	

.aligncenter { text-align:center !important; }
.alignright { text-align:right !important; }
.alignleft { text-align:left !important; }
.alignjustify { text-align:justify !important; }
.floatleft {float: left;}
.floatright {float: right;}

.regular { font-size:1em; }
.large { font-size:1.3em; }
.larger { font-size:2em; }
.xlarge { font-size:3em; }
.small { font-size:0.75em; }
.tiny { font-size:0.6em; }
.bold { font-weight: bold; }
.bolder { font-weight: bolder; }
.white {color: white;}

.zero8 {
	font-size:0.8em;
}
.zero7 {
	font-size:0.7em;
}
.zero6 {
	font-size:0.6em;
}
.zero5 {
	font-size:0.5em;
}
.zero4 {
	font-size:0.4em;
}

.transparent { opacity:0;}
.hidden { display:none !important;}

.noselect {
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}





.largeinput {
	border:solid 3px #369;
	font-size: 1.4em;
	padding: 0.2em !important;
	/* width: 300px; */
	font-weight:bold;
	color:#003;
}
.largebtn {
	font-size: 1.4em;
	padding: 0.7em !important;
}




a {
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	cursor:pointer;

}



