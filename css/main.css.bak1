	#menuwrapper {
		width: 100%;
		margin: 0px auto 0px auto;
		z-index:97;
		height:auto;
			-webkit-touch-callout: none;
			-webkit-user-select: none;
			-khtml-user-select: none;
			-moz-user-select: none;
			-ms-user-select: none;
			user-select: none;
			clear:both;
	
		position:fixed;
		top:0px;	
	}
	

#mainmenu {
	width: 100%;
	height: auto;
	line-height: 1em;
	margin: 0px auto 0px auto;
	/* background-color: #090909; */
	background: #8d8e92;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #8d8e92 1%, #090909 75%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#8d8e92), color-stop(75%,#090909));
	background: -webkit-linear-gradient(top, #8d8e92 1%,#090909 75%);
	background: -o-linear-gradient(top, #8d8e92 1%,#090909 75%);
	background: -ms-linear-gradient(top, #8d8e92 1%,#090909 75%);
	background: linear-gradient(to bottom, #8d8e92 1%,#090909 75%);
	/* filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#8d8e92', endColorstr='#090909',GradientType=0 ); */
	z-index: 98;
	clear: both;
	justify-content: center;
}


#mainmenu ul
{
				display:inline-table;
				margin: 0px 0px 0px 5px;
				padding: 0px 0px 0px 0px;
				z-index:98;
				position: relative;
					box-sizing:border-box;
		 			-moz-box-sizing:border-box; /* Firefox */
}

#mainmenu ul li
{
					font-family: Arial, Helvetica, sans-serif;
					font-size: 1em;
					font-weight: bold;
					color: #F3DD92;
					line-height: 1.5em;
					height:1.5em;
					background-color:transparent;
					display:block;
					float:left;
					border-left: #989898 1px solid;
					border-right: #aba5a5 1px solid;
					padding: 0 .3em;

						 box-sizing:border-box;
		 				-moz-box-sizing:border-box; /* Firefox */
}

#mainmenu ul li a, #mainmenu ul li span
{
			text-decoration:none;
			color:#FFF;
			display:block;
			width:100%;
			padding: 0px 9px 0px 9px;
				 box-sizing:border-box;
		 		-moz-box-sizing:border-box; /* Firefox */
}

#mainmenu ul li a:hover, #mainmenu ul li.active 
{
					background-color:#06607C;
					color:#A7E6E3;
}

/*nest*/
				#mainmenu ul li ul {
					position: absolute;
					top: 100%;
					width:inherit;
					display:none;
					
				}
					#mainmenu ul li:hover ul {
						display: block;
						top:auto;
						
					}
						#mainmenu ul li ul li{
							position:relative;
							float: none;
							display: block;
							background-color:#06607C; /* !!! */
							border-bottom:#000 2px solid;
							border-left:#090909 2px solid;
							border-right:#FFF 2px solid;
							box-shadow: 3px 3px 4px #999;
							cursor:pointer;
						}
						#mainmenu ul li ul li a, #mainmenu ul li ul li span
						{
							text-decoration:none;
							color:#FFF;
							display:inline-block;
							padding: 0px 9px 0px 9px;
						}						
						
						#mainmenu ul li ul li:hover,
						#mainmenu ul li ul li a:hover
						 {
							background-color:#00415E !important;
						}
						
										#mainmenu ul li ul li ul {
											display:none !important;
											list-style:none;
											margin:0px 0px;
											padding:0px 0px;
											/* background-color:#DDD !important; */
											position:absolute;
											left:99%;
											top:0% !important;
											width:inherit;
											/* width:100%; */
											/* z-index: 98; */
											background-color:#eee;
										}
										
										#mainmenu ul li ul li ul li {
											display:inherit;
											/* border: 1px #CCCCCC solid;
											background-color:#eee; */
											background-color:#005370;
											position:relative;
										}
										
										#mainmenu ul li ul li:hover  ul{
											display:block !important;
										}
										

