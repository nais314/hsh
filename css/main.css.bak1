	#fluidmenu {
		background: white;
		display: table;
		flex-direction: column;
		align-items: start;
		align-self: stretch;
		justify-content: start;
		color: #F3DD92;
		max-width: 20em;
		width: 2em;
		flex: 0 auto;

		position: fixed;
		top: .4em;
		right: .6em;
		border-radius: 10px;
    	box-shadow: #777 2px 4px 8px;
    	border: 3px ridge lightgrey;
	}

	._desktop { visibility: hidden; display: none; }

	/* mobile like icon only menubar in overlay */
	#menu-app-icn-m {
		align-items: baseline;
	}

	/* in overlay menu */
 	#menu-action-m ul li, #menu-action-m ul li:hover {
		display: block;
	}


}


@media all and (max-width: 320px) { 
/* styles for narrower desktop browsers and iPad portrait */ 
	body {
		font-size:1.3em;
	}
	#container {
		 width: 100%;
	}
	#menuwrapper {
		width: 100%;
		margin: 0px auto 0px auto;
		z-index:97;
		height:auto;
			-webkit-touch-callout: none;
			-webkit-user-select: none;
			-khtml-user-select: none;
			-moz-user-select: none;
			-ms-user-select: none;
			user-select: none;
			clear:both;
	
		position:static;
		top:0px;	
	}

	#menuwrapper_placeholder {
		display: none;
	}




	
#mainmenu ul
{
				display:inline-block;
				margin: 0px 0px 0px 0px;
				padding: 0px 0px 0px 0px;
				z-index:98;
				position: relative;
				width:100%;
}	
#mainmenu ul li
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1em;
	font-weight: bold;
	color: #FFF;
	line-height: 2em;
	height: 2em;
	background-color: transparent;
	display: block;
	float: none;
	margin: 0px 0px 0px 0px;
	border-left: #0CF 2px ridge;
	border-right: #0CC 2px ridge;
	width: 100%;
	border-radius: 10px;	
}

#mainmenu ul li a, #mainmenu ul li span
{
	text-decoration:none;
	color:#FFF;
	display:block;
	padding: 0px 9px 0px 9px;
	width:100%;
	border-radius: 10px;	
}	




#submenu ul
{
				display:inline-block;
				margin: 0px 0px 0px 0px;
				padding: 0px 0px 0px 0px;
				z-index:97;
				position: relative;
				width:100%;
				
}

#submenu ul li
{
					font-family: Arial, Helvetica, sans-serif;
					font-size: 1em;
					font-weight: bold;
					color: #FFF;
					line-height: 1.5em;
					height:1.5em;
					background-color:transparent;
					display:block;
					float:none;
					margin: 0px 0px 0px 0px;
					cursor:pointer;
					border-left:#333 1px solid;
					border-right:#666 1px solid;
						width: 100%;
						border-radius: 10px;	
}

#submenu ul li a, #submenu ul li span
{
			text-decoration:none;
			color:#FFF;
			display:inline-block;
			padding: 0px 9px 0px 9px;
				width:100%;
				border-radius: 10px;			
}
	
} 